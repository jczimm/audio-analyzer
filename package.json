{
  "name": "audio-analyzer",
  "version": "0.1.1",
  "description": "",
  "main": "app.js",
  "scripts": {
    "postinstall": "bash postinstall.sh",
    "start": "electron ./app",
    "dev": "gulp dev",
    "lint": "gulp lint",
    "clean": "rm -rf ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "clean:win": "rm -rf ./dist/win",
    "build": "npm run build:osx && npm run build:win",
    "build:update": "npm run build:osx:update && npm run build:win:update",
    "_build:osx": "npm run clean:osx && electron-packager ./app \"Audio Analyzer for MAV\" --ignore=\"node_modules/(^electron|^gulp|^babel|^eslint)\" --out=dist/osx --platform=darwin --arch=x64 --version=0.33.0 --icon=assets/osx/audio-analyzer.icns",
    "build:osx": "gulp build --force && npm run _build:osx",
    "build:osx:update": "gulp build && npm run _build:osx",
    "_build:win": "npm run clean:win && electron-packager ./app \"Audio Analyzer for MAV\" --ignore=\"node_modules/(^electron|^gulp|^babel|^eslint)\" --out=dist/win --platform=win32 --arch=ia32 --version=0.33.0 --icon=assets/win/icon.ico",
    "build:win": "gulp build --force && npm run _build:win",
    "build:win:update": "gulp build && npm run _build:win",
    "pack": "npm run pack:osx && npm run pack:win",
    "pack:osx": "electron-builder \"dist/osx/Audio Analyzer for MAV.app\" --platform=darwin --out=\"dist/osx\" --config=packager.json",
    "pack:win": "electron-builder \"dist/win/Audio Analyzer for MAV-win32-ia32\" --platform=win --out=\"dist/win\" --config=packager.json",
    "export": "npm run export:osx && npm run export:win",
    "export:osx": "npm run build:osx && npm run pack:osx",
    "export:win": "npm run build:win && npm run pack:win"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jczimm/audio-analyzer.git"
  },
  "keywords": [
    "audio",
    "analyzer",
    "analyser",
    "frequency",
    "electron"
  ],
  "author": "Jacob Zimmerman <jczimm@jczimm.com> (http://jczimm.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jczimm/audio-analyzer/issues"
  },
  "homepage": "https://github.com/jczimm/audio-analyzer",
  "dependencies": {
    "afa-file": "^1.0.4",
    "fs-extra": "^0.26.2",
    "jquery": "^2.1.4",
    "loop": "^2.1.2",
    "web-audio-analyser": "^2.0.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.3.2",
    "rcedit": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "electron-builder": "^2.3.0",
    "electron-connect": "^0.3.3",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.33.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-changed": "^1.3.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^1.1.1",
    "gulp-if": "^2.0.0",
    "gulp-inject-string": "^1.0.0",
    "gulp-param": "^0.6.4",
    "gulp-rucksack": "^0.1.2",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "stripcolorcodes": "^0.1.0"
  }
}
